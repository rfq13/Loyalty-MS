{
  "name": "expresso",
  "version": "4.0.0",
  "description": "express, sequelize, typescript",
  "main": "./src/server.ts",
  "scripts": {
    "key:generate": "npx ts-node ./src/@expresso/scripts/generate.ts",
    "dev": "yarn && npm-run-all build:ts start:dev",
    "start": "nodemon --exec \"ts-node\" ./src/server.ts",
    "start:dev": "nodemon --exec node ./dist/server.js",
    "clean": "rm -rf ./dist",
    "test:lint": "npx eslint \"*/**/*.{js,jsx,ts,tsx}\" --quiet --fix",
    "test:types": "tsc --noEmit",
    "test:types-watch": "npm run test:lint test:types -- --watch",
    "prebuild": "npm-run-all clean test:types",
    "build": "npx swc src -d dist",
    "build:ts": "npx tsc -p .",
    "serve": "node ./dist/server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate-create": "npx sequelize-cli migration:generate --name",
    "db:rollback": "npx sequelize-cli db:migrate:undo",
    "db:migrate-fresh": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed": "npx sequelize-cli db:seed --seed",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:reset": "yarn && npm-run-all db:drop db:create db:migrate db:seed"
  },
  "author": "rfq13",
  "license": "MIT",
  "engines": {
    "node": ">= 14.x"
  },
  "dependencies": {
    "@types/exceljs": "^1.3.0",
    "@types/express-session": "^1.17.5",
    "bcrypt": "^5.0.1",
    "chalk": "4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-useragent": "^1.0.15",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "https": "^1.0.0",
    "i18next": "^23.10.0",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-middleware": "^3.2.1",
    "ioredis": "^5.2.2",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.10.12",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "randomstring": "^1.2.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "sequelize": "^6.21.3",
    "sequelize-typescript": "^2.1.3",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.5",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/convert-excel-to-json": "^1.7.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/hpp": "^0.2.2",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/ioredis": "^4.28.10",
